{
  "name": "ditc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build",
    "lint": "next lint",
    "setup": "node scripts/setup.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "sitemap:enhanced": "node scripts/enhanced-sitemap.js",
    "test:cdn": "node scripts/test-cdn.js",
    "auto-update:start": "node scripts/auto-update.js start",
    "auto-update:stop": "node scripts/auto-update.js stop",
    "auto-update:update": "node scripts/auto-update.js update",
    "auto-update:status": "node scripts/auto-update.js status",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs ditc-auto-updater",
    "seo:build": "npm run sitemap:enhanced && npm run build",
    "serve": "node scripts/serve-static.js",
    "serve:6001": "node scripts/serve-static.js 6001",
    "clean": "rm -rf .next out node_modules package-lock.json logs .cache",
    "clean:all": "node scripts/clean-rebuild.js",
    "rebuild": "npm run clean && npm install && npm run seo:build && npm run pm2:start",
    "deploy:package": "node scripts/create-deployment-package.js",
    "test:events": "node scripts/test-events-data.js",
    "diagnose:build": "node scripts/diagnose-build.js",
    "fix:build": "node scripts/fix-build-issues.js"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "axios": "^1.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
